{
    "name": "blog-tree/lib-bt-branch-installer",
    "description": "The way for install Blog-Tree extensions in right path",
    "keywords": ["bt", "bt-composer", "bt-lib-path", "Blog-Tree", "bt-branch"],
    "license": "MIT",
    "type": "composer-plugin",
    "minimum-stability": "dev",
    "support": {
        "forum": "http://blog-tree.com",
        "issues": "http://blog-tree.com"
    },
    "homepage": "http://blog-tree.com",
    "require": {
        "composer-plugin-api": "^1.0"
    },
    "autoload": {
        "psr-0": {
            "bt\\composer": "./"
        }
    },
    "extra": {
        "class": "bt\\composer\\BlogTreeInstallerPlugin"
    },
    "authors": [
        {
            "name": "Alexander Zagovorichev",
            "email": "zagovorichev@gmail.com"
        }
    ]
}
